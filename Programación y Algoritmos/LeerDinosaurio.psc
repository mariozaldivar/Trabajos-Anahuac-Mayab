Algoritmo LeerDinosaurio
	Definir especie Como Caracter;
	Definir largo, peso Como Real;
	
	Escribir "Introduzca el nombre de la especie";
	Leer especie;
	Escribir "Introduzca la longitud de la especie EN PIES";
	Leer largo;
	Escribir "Introduzca el peso de la especie EN TONELADAS";
	Leer peso;
	
	Escribir "La especie ", especie, " tiene una longitud de ", largo * .3047, "metros, y un peso de ", peso * 1000, "kg.";
	
	
	
FinAlgoritmo
